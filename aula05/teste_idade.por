programa {
  funcao inicio() {
    inteiro idade
    idade = 16
    se (idade >= 18) {
      escreva("Você é maior de idade.\n")
      escreva("Já pode tirar carta de motorista.\n")
    }
    se (idade < 18) {
      escreva("Você é menor de idade.\n")
      escreva("Já não pode tirar carta de motorista.\n")
    }
    escreva("Fim do Programa")
  }
}
